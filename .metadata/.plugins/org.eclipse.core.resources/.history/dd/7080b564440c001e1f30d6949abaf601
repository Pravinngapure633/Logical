import java.util.Scanner;

public class SumOfNumber {
	
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int sum=0;
		System.out.println("Enter A number");
		int num=sc.nextInt();
	
		for(int i=0;i<=num;i++)
		{
			sum+=i;
			System.out.println(sum+""+i);
		}
		System.out.println(sum);
	}

}
